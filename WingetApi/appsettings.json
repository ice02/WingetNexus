{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "RootUrl": "https://localhost:5001",
  "ConnectionStrings": {
    "WingetSqLiteContext": "Data Source=winget.db"
  },
  "DatabaseType": "SQLite",
  "Reponame": "LocalTest",
  "BaseUrl": "https://localhost:5001",
  "FeatureManagement": {
    "RequireAuthentication": false,
    "HealthChecks": false,
    "Metrics": false,
    "S3Storage": false
  },
  "S3StorageSettings": {
    "BucketName": ""
  },
  "LocalStorageSettings": {
    "BasePath": "C:\\temp\\winget\\"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/applog-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "Properties": {
      "ApplicationName": "Winget Nexus client Api"
    }
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "Winget Nexus client Api Health Checks",
        "Uri": "/health"
      }
    ],
    "EvaluationTimeInSeconds": 5
  },
  "easycaching": {
    "redis": {
      "MaxRdSecond": 120,
      "EnableLogging": false,
      "LockMs": 5000,
      "SleepMs": 300,
      "dbconfig": {
        "Password": null,
        "IsSsl": false,
        "SslHost": null,
        "ConnectionTimeout": 5000,
        "AllowAdmin": true,
        "Endpoints": [
          {
            "Host": "localhost",
            "Port": 6739
          }
        ],
        "Database": 0
      }
    }
  }
}
